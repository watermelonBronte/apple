	<package name="asset_category" extends="struts-default"
		namespace="/asset_category">
		<action name="test_*_*" class="{1}Action" method="{2}">
			<result name="findAssetCategoryById">/jsp/assetCategory/asset_category_update.jsp
			</result>
			<result name="pageAssetCategory">/jsp/assetCategory/asset_category_page.jsp
			</result>
			<result name="successAssetCategory">/jsp/assetCategory/asset_category_success.jsp
			</result>
			<result name="addAssetCategory">/jsp/assetCategory/asset_category_add.jsp
			</result>
			<result name="home">/index.jsp</result>
		</action>
	</package>

